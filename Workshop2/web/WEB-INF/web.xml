<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoadCategory</servlet-name>
        <servlet-class>controller.LoadCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>controller.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadItem</servlet-name>
        <servlet-class>controller.LoadItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendFAQ</servlet-name>
        <servlet-class>controller.SendFAQ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PopUp</servlet-name>
        <servlet-class>controller.PopUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoadCategory</servlet-name>
        <url-pattern>/LoadCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/MainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadItem</servlet-name>
        <url-pattern>/LoadItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendFAQ</servlet-name>
        <url-pattern>/SendFAQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PopUp</servlet-name>
        <url-pattern>/PopUp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MainController</welcome-file>
    </welcome-file-list>
</web-app>
